{
  "name": "config",
  "version": "1.0.0",
  "description": "personal config files",
  "scripts": {
    "bkp-spacemacs": "cp ~/.spacemacs ./",
    "restore-spacemacs": "mv ~/.spacemacs ~/.spacemacs_bkp && cp .spacemacs ~/.spacemacs",
    "bkp-zshrc": "cp ~/.zshrc ./",
    "restore-zshrc": "mv ~/.zshrc ~/.zshrc_bkp && cp .zshrc ~/.zshrc",
    "bkp-bashrc": "cp ~/.bashrc ./",
    "restore-bashrc": "mv ~/.bashrc ~/.bashrc_bkp && cp .bashrc ~/.bashrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vedovato/spacemacs-setup.git"
  },
  "author": "Andrei Vedovato",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vedovato/spacemacs-setup/issues"
  },
  "homepage": "https://github.com/vedovato/spacemacs-setup#readme"
}
